{
  "name": "wallet-api",
  "version": "0.0.1",
  "description": "Wallet RESTful API Project",
  "main": "dist",
  "scripts": {
    "dev": "SET NODE_ENV=development & nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "SET NODE_ENV=production & pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1      
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbali22/wallet"
  },
  "author": "Mutturaj Bali",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.9.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "aws-sdk": "^2.307.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "path": "^0.12.7"
  }
}
